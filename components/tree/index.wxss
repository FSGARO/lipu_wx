

.tree_container {
  background:#fff;
  /* border-bottom: 1rpx solid rgba(0,0,0,.1); */
}
.nocollapse_icon { /*展开图片*/
  transform: rotate(90deg);
}
.tree_text {
  display: inline-block;
  vertical-align: middle;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 90%;
}
.treeArrItem{
  padding:6rpx 30rpx;
  position: relative;
}
.treeArrItem .tip{
  position: absolute;
  font-size: 28rpx;
  color: rgb(33, 40, 146);
  left:-10rpx;
  top:16rpx;
  width: 40rpx;
  height: 40rpx;
  text-align: center;

}