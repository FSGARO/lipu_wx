.tab{
  height: 100rpx;
  background-color: var(--cfff);
  box-shadow: 8rpx 0rpx 8rpx 0rpx #ccc;

}
.tabbtb{
  width: 100%;
  height: 3rpx;
  background-color: var(--bglabel);
  position: absolute;
  bottom: 0;
  left: 0;
}

.con{
  margin-top: 30rpx;
}

.con .li{
  background-color: var(--cfff);
  padding:20rpx 30rpx;
}

.active{
  color: var(--bglabel);
}

.radio-group{
  padding-top: 15rpx;
 
}

.radio-group label{
  font-size: 30rpx;
}

.offli{
  padding: 10rpx 0;
  border-bottom: 2rpx solid #ccc;
}

