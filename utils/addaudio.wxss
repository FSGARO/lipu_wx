  margin-left: 20rpx;
} */

.pl60 {
  padding-left: 40rpx !important;
}

.pr60 {
  padding-right: 60rpx !important;
}

.pr40 {
  padding-right: 40rpx !important;
}

.selectweek {
  padding-right: 0;
}

.selectweek input{
  font-size: 32rpx;
  word-break: break-all;
  word-wrap: break-word;
  width: 100%;
}

.selectweek .val {
  flex: 1;
  margin-right: 6rpx;
  height: 60rpx;
}
