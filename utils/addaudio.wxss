.searchSolt .seach {
  width: 90%;
  margin: 20rpx auto;
  /* position: relative; */
}

.thead {
  height: 106rpx;
  line-height: 106rpx;
  background-color: var(--theadbg);
  color: var(--cfff);
}

.lable-con .rol1 {
  width: 130rpx;
}

.thead .title {
  width: 200rpx;
  border-right-width: 2rpx;
  border-right-style: solid;
  border-right-color: #ccc;
}

.num {
  width: 55rpx;
  height: 55rpx;
  line-height: 55rpx;
  text-align: center;
  border-radius: 50%;
  background-color: var(--bglabel);
  color: var(--cfff);
}

.con .li .title {
  width: 200rpx;
}

.con .li {
  height: 130rpx;
  border-bottom: 1rpx solid #ccc;
  margin-left: 30rpx;
}

.icon image {
  width: 60rpx;
}

.batch {
  padding: 0 15rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 15rpx;
  background-color: var(--addbtnbg);
  color: var(--cfff);
}

.batchControl {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 80%;
  height: 500rpx;
  background-color: var(--seatbox);
  border-radius: 20rpx;
  top: 200rpx;
  padding: 20rpx 30rpx;
  z-index: 555;
}

.batchControl .title {
  padding: 30rpx 0;
  text-align: center;
}

.batchControl .li .icon {
  width: 150rpx;
  height: 150rpx;
  background-color: #999;
  border-radius: 50%;
}

.batchControl .li .icon image {
  width: 40%;
}

.conWrap {
  background-color: var(--cfff);
  border-radius: 15rpx;
}

.conWrap .con .li {
  border-bottom: none;
}

.batchControl .close {
  width: 80rpx;
  position: absolute;
  bottom: -50rpx;
  left: 0;
  right: 0;
  margin: auto;
}

.descBtn {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  padding: 5rpx;
  color: #ccc;
}

.pickerView {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: var(--cfff);
  z-index: 555;
}

.priority {
  padding: 20rpx 50rpx 20rpx 60rpx;
  background-color: var(--cfff);
}

.addaudio {
  margin-bottom: 230rpx;
}

.resource-lable, .label-time {
  padding: 30rpx 40rpx;
}

.label-repeat, .lable-common,.label-time{
  padding:10rpx 40rpx;
}

.cycle .rol1 {
  width: 190rpx;
  padding-left: 25rpx;
}

.cycle  text {
  color: #888;
}

